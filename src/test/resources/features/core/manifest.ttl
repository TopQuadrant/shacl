<class-001>
	a sht:Validate ;
	mf:name "Test of a sh:ShapeClass definition" ;
	mf:action [
		sht:schema <class-001.ttl> ; 
		sht:data <class-001.ttl> ; 
	] ;
	mf:result [
		a sh:Error ;
  		sh:root _:b1 ;
  		sh:subject _:b1 ;
  		sh:predicate sh:maxCount ;
  		sh:object 1.2 ;
	] ;
	mf:result [
		a sh:Error ;
  		sh:root _:b1 ;
  		sh:subject _:b1 ;
  		sh:predicate sh:predicate ;
	] ;
	mf:status sht:proposed ; 
.

