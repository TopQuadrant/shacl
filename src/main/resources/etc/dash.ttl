  sh:nodeValidator [
      rdf:type sh:SPARQLSelectValidator ;
      sh:message "Value does have shape {$not}" ;
      sh:sparql """
		SELECT $this ?failure
		WHERE {
			BIND (sh:hasShape($this, $not, $shapesGraph) AS ?hasShape) .
			BIND (!bound(?hasShape) AS ?failure) .
			FILTER (?failure || ?hasShape) .
		}
		""" ;
    ] ;
