@prefix sh:    <http://www.w3.org/ns/shacl/core#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
 
<begin>  a           sh:OpenShape ;
        sh:group     [ sh:minCount  1 ;
                       sh:property  [ a                 sh:PropertyConstraint ;
                                      sh:allowedValues  ( 1 ) ;
                                      sh:minCount       1 ;
                                      sh:maxCount       1 ;
                                      sh:predicate      <b>
                                    ] ;
                       sh:property  [ a                 sh:PropertyConstraint ;
                                      sh:allowedValues  ( 1 ) ;
                                      sh:minCount       1 ;
                                      sh:maxCount       1 ;
                                      sh:predicate      <a>
                                    ]
                     ] ;
        sh:property  [ a                 sh:PropertyConstraint ;
                       sh:allowedValues  ( 1 ) ;
                       sh:minCount       1 ;
                       sh:maxCount       1 ;
                       sh:predicate      <c>
                     ] ;
        sh:schema    [ a  sh:Schema ] .